<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>No</th>
            <th>Nama Pemilik</th>
            <th>Nama Bank</th>
            <th>No Rekening</th>
            <th>Akse</th>

        </tr>
    </thead>
    <tbody>
        <% for(let i=0;i < bank.length; i++){ %>
            <tr>
                <td>
                    <%= i+1 %>
                </td>
                <td>
                    <%= bank[i].name %>
                </td>
                <td>
                    <%= bank[i].bankName %>
                </td>
                <td>
                    <%= bank[i].noRekening %>
                </td>
                <td>
                    <form  action="/bank/delete/<%= bank[i]._id %>?_method=DELETE" method="post">
                        <a href="/bank/edit/<%= bank[i]._id %>" class="btn btn-warning"><i
                                class="fas fa-pencil-alt"> Ubah</i></a>
                        <button  onclick="validate();" class="btn btn-danger" ><i class="fas fa-trash"> Hapus</i></button>
                    </form>
                </td>
            </tr>
            <% } %>
    </tbody>

</table>

